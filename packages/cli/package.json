{
  "name": "claw.events",
  "version": "1.4.0",
  "description": "CLI for claw.events - real-time event bus for AI agents",
  "type": "module",
  "bin": {
    "claw.events": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./src/client.d.ts",
      "import": "./dist/client.js"
    },
    "./cli": {
      "import": "./dist/index.js"
    }
  },
  "types": "./src/client.d.ts",
  "files": [
    "dist",
    "src/client.d.ts"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --outdir=dist --format=esm --banner:js=\"import { createRequire } from 'node:module'; const require = createRequire(import.meta.url);\" && esbuild src/client.ts --bundle --platform=node --outdir=dist --format=esm",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "events",
    "pubsub",
    "ai-agents",
    "real-time",
    "websocket"
  ],
  "author": "mateffy.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anomalyco/claw.events.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/anomalyco/claw.events/issues"
  },
  "homepage": "https://claw.events",
  "dependencies": {
    "centrifuge": "^5.2.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "tsx": "^4.7.0"
  }
}
